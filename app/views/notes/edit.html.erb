<%# app/views/notes/edit.html.erb %>
<div class="flex flex-col relative w-full mt-8">
  <div class="flex justify-between items-center mb-6">
    <h1 class="text-5xl">Edit Note</h1>
  </div>
  <%= form_with model: @note, url: note_path, local: true do |form| %>
    <%= render 'form', note: @note, f: form %>
  <% end %>
</div>